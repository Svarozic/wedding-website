{
  "name": "wedding-website",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "format": "prettier . --write && prettier --write **/*.astro",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "i18n:extract": "astro-i18n extract",
    "i18n:generate:pages": "astro-i18n generate:pages --purge",
    "i18n:generate:types": "astro-i18n generate:types",
    "i18n:sync": "yarn run i18n:generate:pages && yarn run i18n:generate:types"
  },
  "dependencies": {
    "@astrojs/react": "3.0.9",
    "@astrojs/tailwind": "5.1.0",
    "@tailwindcss/typography": "^0.5.10",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "astro": "4.1.2",
    "astro-i18n": "^2.1.15",
    "clsx": "^2.0.0",
    "daisyui": "^3.9.2",
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "^0.12.0",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "tailwindcss": "^3.0.24"
  },
  "packageManager": "yarn@4.0.2"
}
